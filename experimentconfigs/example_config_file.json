{
    "configuration": {
        "client": {
            "ycsb_replicas": 1,
            "ycsb_thread_count": 16,
            "ycsb_record_count": 1000,
            "ycsb_operation_count": 10000,
            "workload_file": "workloada"
        },
        "target": {
            "cassandra_replicas": 2,
            "cassandra_replication_factor": 1,
            "cassandra_parameters": {}
        },
        "search_algorithm": {
            "initialization": {
              "generation_strategy": "sobol15_gpei"
            },
            "type": "axsearch",
            "num_samples": 1,
            "objective": {
                "name": "OVERALL_Throughput(ops/sec)",
                "direction": "max"
            }
        }
    },
    "parameters": [
        {
            "name": "cassandra:row_cache_size_in_mb",
            "type": "range",
            "bounds": [2, 256],
            "value_type": "int",
            "log_scale": false
        }
    ]
}